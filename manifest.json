{
		"manifest_version": 3,
		"name": "ALX README maker",
		"version": "2.1",
		"description": "make README file for alx projects",
		"permissions": [
				"activeTab",
				"scripting"
		],
		"background": {
				"service_worker": "background.js"
		},
		"action": {},
		"icons": {
				"16": "icon16.png",
				"32": "icon32.png",
				"48": "icon48.png",
				"128": "icon128.png"
		},
		"host_permissions": [
				"https://intranet.alxswe.com/projects/*"
		]
}
